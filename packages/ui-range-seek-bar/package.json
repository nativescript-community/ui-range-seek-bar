{
    "name": "@nativescript-community/ui-range-seek-bar",
    "version": "2.0.1",
    "description": "NativeScript Range Seek Bar widget",
    "main": "./index",
    "sideEffects": false,
    "typings": "./index.d.ts",
    "scripts": {
        "build": "npm run tsc && npm run readme",
        "build.all": "npm run build && npm run build.angular",
        "build.angular": "../../node_modules/.bin/ng-packagr -p ../../src/ui-range-seek-bar/angular/ng-package.json -c ../../src/ui-range-seek-bar/angular/tsconfig.json",
        "readme": "../../node_modules/.bin/readme generate -c ../../tools/readme/blueprint.json",
        "tsc": "../../node_modules/.bin/cpy '**/*.d.ts' '../../packages/ui-range-seek-bar' --parents --cwd=../../src/ui-range-seek-bar && ../../node_modules/.bin/tsc -skipLibCheck -d",
        "clean": "../../node_modules/.bin/rimraf ./*.d.ts ./*.js ./*.js.map"
    },
    "nativescript": {
        "platforms": {
            "android": "6.0.0",
            "ios": "6.0.0"
        }
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS",
        "NativeScript UI",
        "menu",
        "nativescript-community",
        "Angular",
        "Vue",
        "Svelte",
        "React",
        "preview|https://raw.githubusercontent.com/nativescript-community/ui-range-seek-bar/master/images/demo-ios.gif|iOS Demo",
        "preview|https://raw.githubusercontent.com/nativescript-community/ui-range-seek-bar/master/images/demo-android.gif|Android Demo"
    ],
    "author": {
        "name": "JK",
        "email": "lexuanhoang2506@gmail.com"
      },
    "bugs": {
        "url": "https://github.com/nativescript-community/ui-range-seek-bar/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nativescript-community/ui-range-seek-bar"
    },
    "license": "Apache-2.0",
    "readmeFilename": "README.md"
}
