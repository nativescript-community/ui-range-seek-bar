{
  "name": "@nativescript-community/ui-range-seek-bar",
  "version": "2.0.1",
  "description": "NativeScript Range Seek Bar widget",
  "main": "range-seek-bar",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "7.0.0",
      "ios": "7.0.0"
    }
  },
  "scripts": {
    "setup": "ts-patch install",
    "tsc": "tsc -skipLibCheck",
    "build": "tsc && ngc",
    "postclone": "npm i && node scripts/postclone.js && cd ../demo && npm i && cd ../src && npm run plugin.link",
    "tslint": "cd .. && tslint \"**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\"",
    "plugin.link": "npm link && cd ../demo && npm link nativescript-auto-complete-text-field && cd ../src",
    "plugin.tscwatch": "npm run tsc -- -w",
    "demo.ios": "npm i && npm run tsc && cd ../demo && ns run ios --syncAllFiles",
    "demo.android": "npm i && npm run tsc && cd ../demo && ns run android --syncAllFiles",
    "demo.ios.ng": "npm i && npm run tsc && cd ../demo-ng && ns run ios --syncAllFiles",
    "demo.android.ng": "npm i && npm run tsc && cd ../demo-ng && ns run android --syncAllFiles",
    "demo.reset": "cd ../demo && rimraf platforms",
    "plugin.prepare": "npm run tsc && cd ../demo && ns plugin remove nativescript-auto-complete-text-field && ns plugin add ../src",
    "clean": "cd ../demo && rimraf hooks node_modules platforms && cd ../src && rimraf node_modules && npm run plugin.link",
    "ci.tslint": "npm i && tslint '**/*.ts' --config '../tslint.json' --exclude '**/node_modules/**'",
    "ngc": "ngc --project tsconfig.aot.json",
    "generate.declaration.ios": "cd ../demo-ng && TNS_TYPESCRIPT_DECLARATIONS_PATH=\"$(pwd)/typings\" ns build ios"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Android",
    "iOS"
  ],
  "author": {
    "name": "JK",
    "email": "lexuanhoang2506@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/HoangJK/nativescript-range-seek-bar/issues"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/HoangJK/nativescript-range-seek-bar",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@angular/animations": "13.3.3",
    "@angular/common": "13.3.3",
    "@angular/compiler": "13.3.3",
    "@angular/compiler-cli": "13.3.3",
    "@angular/core": "13.3.3",
    "@angular/forms": "13.3.3",
    "@angular/platform-browser": "13.3.3",
    "@angular/platform-browser-dynamic": "13.3.3",
    "@angular/router": "13.3.3",
    "@nativescript/angular": "13.0.3",
    "@nativescript/core": "8.2.2",
    "@nativescript/types": "8.2.0",
    "@nativescript/webpack": "~5.0.6",
    "rxjs": "~7.5.5",
    "ts-patch": "^2.0.1",
    "tslint": "~5.11.0",
    "typescript": "~4.6.3",
    "zone.js": "~0.11.5"
  },
  "bootstrapper": "nativescript-plugin-seed",
  "dependencies": {
    "ts-node": "^10.7.0"
  }
}
